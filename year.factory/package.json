{
  "name": "growr-rewards",
  "version": "0.1.0",
  "description": "ggrowr rewards",
  "scripts": {
    "types": "npx hardhat typechain",
    "deploy-private": "npm run types && ENV_FILE_PATH=./.env.private npx hardhat run  --network theta_privatenet scripts/deploy.ts",
    "deploy-test": "npm run types && ENV_FILE_PATH=./.env.test npx hardhat run  --network theta_testnet scripts/deploy.ts",
    "upgrade-private": "npm run types && ENV_FILE_PATH=./.env.private npx hardhat run  --network theta_privatenet scripts/upgrade.ts",
    "upgrade-test": "npm run types && ENV_FILE_PATH=./.env.test npx hardhat run  --network theta_testnet scripts/upgrade.ts",
    "node": "cd /home/nuc/theta_local_privatenet_linux/bin && ./theta start --config=../privatenet/validator --password=qwertyuiop",
    "rpc": "cd /home/nuc/theta_local_privatenet_linux/bin && ./theta-eth-rpc-adaptor start --config=../privatenet/eth_rpc_adaptor",
    "check": "curl -X POST -H 'Content-Type: application/json' --data '{\"jsonrpc\":\"2.0\",\"method\":\"eth_chainId\",\"params\":[],\"id\":67}' http://127.0.0.1:18888/rpc",
    "test": "NODE_ENV=test npx hardhat watch test",
    "log": "nodemon",
    "watch": "npx onchange 'contracts/**/*.sol' -- npm run prettier",
    "extract": "node extractAbiAndBytecode.js",
    "prettier": "prettier --write --plugin=prettier-plugin-solidity 'contracts/**/*.sol'"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.8",
    "@nomicfoundation/hardhat-ethers": "^3.0.8",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.12",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.12",
    "@nomiclabs/hardhat-truffle5": "^2.0.7",
    "@nomiclabs/hardhat-web3": "^2.0.1",
    "@openzeppelin/hardhat-upgrades": "^3.9.0",
    "@openzeppelin/test-helpers": "^0.5.16",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/node": "^22.12.0",
    "concurrently": "^9.1.2",
    "dotenv": "^16.4.7",
    "ethers": "^6.13.5",
    "hardhat": "^2.22.18",
    "hardhat-watcher": "^2.5.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "prettier-plugin-solidity": "^1.4.2",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.2",
    "web3": "^1.10.4"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.2.0",
    "@openzeppelin/contracts-upgradeable": "^5.2.0",
    "@types/fs-extra": "^11.0.4",
    "ethers-decode-error": "^2.1.3",
    "fs-extra": "^11.3.0"
  }
}
