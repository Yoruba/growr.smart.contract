    "theta-deploy-test": "ENV_FILE_PATH=./.env.test npx hardhat run  --network theta_testnet scripts/deploy.ts",
    "theta-node": "cd /home/nuc/theta_local_privatenet_linux/bin && ./theta start --config=../privatenet/validator --password=qwertyuiop",
